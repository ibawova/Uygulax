<settings> 
  <category>
    <setting default="0" id="locale" label="Text Sprache" type="enum" values="Deutsch|Englisch" />
    <setting default="1" id="language" label="Media Sprache" type="enum" values="Alle|Deutsch|Englisch" />
    <setting default="0" id="resolution" label="Nur Titel mit dieser Videoauflösung anzeigen" type="enum" values="Alle|HD|SD" />
