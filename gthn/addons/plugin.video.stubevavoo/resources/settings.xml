    <!--<setting default="0" id="stream_select" label="Stream Auswahl" type="enum" values="Automatisch|Gruppiert|Nach Hoster" />-->
    <setting default="0" id="stream_select" label="Stream Auswahl" type="enum" values="Hoster|Automatisch|Gruppiert" />
    <setting default="true" id="auto_try_next_stream" type="bool" subsetting="true" visible="eq(-1,0)" label="Automatisch nächsten ähnlichen Stream versuchen"/>
  </category>
